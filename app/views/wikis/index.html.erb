<h1>Wiki</h1>
<main role="main" class="container">
  <% if user_is_authorized_to_create? %>
  <div class="row">
     <div class="col-md-4">
         <%= link_to "Make a New Wiki", new_wiki_path, class: 'btn btn-success' %>
     </div>
  </div>
  <% end %>
 <% @wikis.each do |wiki| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <!--<% if wiki.private? %>-->
         <!--  <%= link_to wiki.title, wiki, class: "private" %> <small>[private]</small>-->
         <!--<% else %>-->
         <!--  <%= link_to wiki.title, wiki %>-->
         <!--<% end %>-->
          <% if current_user.premium? || current_user.admin? %>
            <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
            </h4>
          <% elsif wiki.private == false %>
            <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
            </h4>
          <% end %>
       </h4>
     </div>
   </div>
 <% end %>
