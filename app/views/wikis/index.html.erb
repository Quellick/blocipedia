<h1>Wiki</h1>
<main role="main" class="container">
  <div class="row">
     <div class="col-md-4">
         <%= link_to "Make a New Wiki", new_wiki_path, class: 'btn btn-success' %>
     </div>
  </div>
 <% @wikis.each do |wiki| %>
    <div class="blog-post-title">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
        </h4>
      </div>
    </div>
 <% end %>
